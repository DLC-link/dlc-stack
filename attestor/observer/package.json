  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.3.1",
    "attestor": "file:../pkg",
    "cross-fetch": "^3.1.6",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "strftime": "^0.10.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/strftime": "^0.9.4",
    "tsc-watch": "^6.0.4"
  }
